# DeathAIAUS Platform Configuration
NODE_ENV=development
PORT=3000

# Database
MONGODB_URI=mongodb://localhost:27017/deathaiaus
MONGODB_ATLAS_URI=your_mongodb_atlas_connection_string

# JWT Secret
JWT_SECRET=your_super_secret_jwt_key_here
JWT_EXPIRES_IN=7d

# Domain Configuration
DOMAIN=www.deathaiaus.com.au
FRONTEND_URL=http://localhost:3000

# AI Integration
LMSTUDIO_URL=http://127.0.0.1:6668
OPENAI_API_KEY=your_openai_api_key_here
DEFAULT_AI_PLATFORM=lmstudio

# YouTube API (for music system)
YOUTUBE_API_KEY=your_youtube_api_key_here
YOUTUBE_PLAYLIST_ID=your_playlist_id_here

# Security
BCRYPT_ROUNDS=12
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100

# WebRTC Configuration
STUN_SERVER=stun:stun.l.google.com:19302
TURN_SERVER=your_turn_server_here
TURN_USERNAME=your_turn_username
TURN_CREDENTIAL=your_turn_credential